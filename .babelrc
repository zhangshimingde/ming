{
    "presets": [
      [
        "env",
        {
          "modules": false,
          "useBuiltIns": true,
          "loose": true,
          "targets": {"browsers": ["safari >= 7", "ie>=9"]}
        }
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      "dva-hmr",
      "transform-runtime",
      "transform-decorators-legacy",
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ]
    ]
  }